<h1>SchoolClasses#new</h1>
<p>Find me in app/views/students/new.html.erb</p>

<h3>School Class Form</h3>

<%= form_for(@class) do |c| %>
  <label>Class Name:</label><br>
  <%= c.text_field :title %><br>

  <label>Room Number:</label><br>
  <%= c.number_field :room_number %><br>

  <%= c.submit %>
<% end %>
